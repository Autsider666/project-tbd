		"@types/node": "^18.0.4",
		"@types/socket.io": "^3.0.2",
		"express": "^4.18.1",
		"socket.io": "^4.5.1",
		"ts-node-dev": "^2.0.0"
	},
	"devDependencies": {
		"@socket.io/admin-ui": "^0.4.0",
